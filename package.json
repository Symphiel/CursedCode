{
    "name": "cursedcode",
    "displayName": "CursedCode",
    "description": "Cursed custom maps.",
    "prestart": "mod.js",
    "version": "0.1.1",
    "module": true,
    "ccmodDependencies": {
    },
    "scripts": {
        "format": "npm run minify-json && npm run json-beautify && npm run js-beautify",
        "minify-json": "minify-json ./",
        "js-beautify": "js-beautify -js -q -r -f ./**/*.js",
        "json-beautify": "js-beautify -js -r -f ./**/*.json"
    },
    "dependencies": {
        "js-beautify": "^1.10.3",
        "minify-json": "^1.0.0"
    }
}
